        <param name="x" type="float" precision="2" max="9999" gui-text="x" gui-description="inner width in mm">100</param>
        <param name="sy" type="string" gui-text="sy" gui-description="sections back to front in mm [ðŸ›ˆ](https://florianfesti.github.io/boxes/html/usermanual.html#section-parameters)">50:50</param>

        <param name="Clamping" type="bool" gui-text="Clamping" gui-description="add clamping holes">false</param>
        <param name="ClampingSize" type="float" precision="2" max="9999" gui-text="ClampingSize" gui-description="diameter of clamping holes">25.0</param>
        <param name="Mounting" type="bool" gui-text="Mounting" gui-description="add mounting holes">false</param>
        <param name="Sturdy" type="bool" gui-text="Sturdy" gui-description="create sturdy construction (e.g. shelf, clamping jig, ...)">false</param>

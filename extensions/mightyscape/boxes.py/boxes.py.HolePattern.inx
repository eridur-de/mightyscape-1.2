        <param name="labels" type="bool" gui-text="labels" gui-description="label the parts (where available)">true</param>
        <param name="reference" type="float" precision="2" max="9999" gui-text="reference" gui-description="print reference rectangle with given length (in mm)(zero to disable) [ðŸ›ˆ](https://florianfesti.github.io/boxes/html/usermanual.html#reference)">100</param>

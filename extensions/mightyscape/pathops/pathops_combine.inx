<?xml version="1.0" encoding="UTF-8"?>
<inkscape-extension xmlns="http://www.inkscape.org/namespace/inkscape/extension">
    <name>Combine</name>
    <id>fablabchemnitz.de.pathops_combine</id>
    <param name="ink_verb" gui-hidden="true" type="string" gui-text="Path operation">path-combine</param>
    <effect needs-document="true" needs-live-preview="false">
        <object-type>all</object-type>
        <effects-menu>
            <submenu name="MightyScape">
                <submenu name="PathOps"/>
            </submenu>
        </effects-menu>
        <menu-tip>Apply Inkscape 'Combine' path operation to multiple objects.</menu-tip>
    </effect>
    <script>
        <command location="inx" interpreter="python">pathops.py</command>
    </script>
</inkscape-extension>